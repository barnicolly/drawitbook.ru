{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8",
        "artesaos/seotools": "^0.19.1",
        "atehnix/vk-client": "^1.3",
        "bensampo/laravel-enum": "^3.4",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "fideloper/proxy": "^4.4",
        "foolz/sphinxql-query-builder": "^3",
        "fruitcake/laravel-cors": "^1.0",
        "htmlmin/htmlmin": "^8.0",
        "laravel/framework": "^8",
        "laravel/ui": "^3",
        "mariuzzo/laravel-js-localization": "^1.7",
        "mik009/sanitizer": "1.2.0",
        "mik009/translation": "2.6",
        "spatie/data-transfer-object": "^3.3",
        "spatie/laravel-sitemap": "^5.8",
        "tightenco/collect": "^8.34"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.8",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5",
        "phpunit/phpunit": "^9"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mik009/translation.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mik009/sanitizer.git"
        }
    ],
    "scripts": {
        "ide": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models -N"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
